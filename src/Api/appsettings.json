{
    "ApplicationName": "ServiceChargingSystem",
    "ApplicationVersion": "0.0.1",
    "AspNetCoreInstrumentation": {
        "RecordException": "true"
    },
    "NacosConfig": {
        "Listeners": [
            {
                "Optional": true,
                "DataId": "Serilog",
                "Group": "BhcMsGeneral"
            },
            {
                "Optional": true,
                "DataId": "Zipkin",
                "Group": "BhcMsServiceChargingSystem"
            },
            {
                "Optional": true,
                "DataId": "ExternalJWTSettings",
                "Group": "BhcMsGeneral"
            },
            {
                "Optional": true,
                "DataId": "InternalJWTSettings",
                "Group": "BhcMsGeneral"
            },
            {
                "Optional": true,
                "DataId": "DatabaseSettings",
                "Group": "BhcMsServiceChargingSystem"
            },
            {
                "Optional": true,
                "DataId": "Features",
                "Group": "BhcMsServiceChargingSystem"
            }
        ],
        "ConfigUseRpc": false,
        "InstanceEnabled": true,
        "DefaultTimeOut": 15000,
        "ListenInterval": 1000,
        "Ephemeral": true
    }
}
